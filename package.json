{
  "version": "5.0.0",
  "config": {
    "outputpath": "orgtheme_jasmin/static/orgtheme/"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "bootswatch": "^5.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.6",
    "grunt": "^1.4.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.1"
  },
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss/main.scss -o ${npm_package_config_outputpath}/${npm_package_version}",
    "css-prefix": "postcss --replace ${npm_package_config_outputpath}/${npm_package_version}/main.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix"
  }
}
